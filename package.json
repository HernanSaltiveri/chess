{
  "name": "wasabi-chess",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://mhonert.github.io/chess",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "react": "^16.12.0",
    "react-dnd": "^9.5.1",
    "react-dnd-html5-backend": "^9.5.1",
    "react-dnd-touch-backend": "^9.5.1",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.1",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "asbuild": "asc assembly/index.ts --validate --noAssert --optimizeLevel 3 --shrinkLevel 0 --converge --use IS_WASI=0 -b public/as-api.wasm",
    "build:engine": "asc assembly/standalone.ts --sourceMap --runtime half --validate --optimizeLevel 3 --shrinkLevel 0 --converge --use IS_WASI=1 --use abort=_abort -b build/engine.wasm -t build/engine.wat",
    "asbuild:debug": "asc assembly/index.ts --debug --use IS_WASI=0 -b public/as-api.wasm",
    "build": "npm run asbuild && npm run jsbuild",
    "jsbuild": "react-scripts build",
    "jsstart": "react-scripts start",
    "start": "npm run asbuild && npm run jsstart",
    "start:debug": "npm run asbuild:debug && npm run jsstart",
    "jstest": "react-scripts test",
    "test": "asp",
    "perft:full": "asp --nortrace -c as-pect-perft.config.js --test='.*#full.*'",
    "perft:quick": "asp --nortrace -c as-pect-perft.config.js --test='.*#quick.*'",
    "perft:engine": "asp --nortrace -c as-pect-perft.config.js --test='.*#engine.*'",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@as-pect/assembly": "^2.8.0",
    "@as-pect/cli": "^2.8.0",
    "@as-pect/core": "^2.8.0",
    "assemblyscript": "0.9.1-nightly.20200210",
    "babel-plugin-macros": "^2.8.0",
    "gh-pages": "^2.2.0",
    "workerize-loader": "^1.1.0"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
