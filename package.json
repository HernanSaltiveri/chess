{
  "name": "wasabi-chess",
  "version": "1.1.7",
  "private": true,
  "homepage": "https://mhonert.github.io/chess",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "react": "^16.13.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dnd-touch-backend": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "asbuild": "asc assembly/index.ts --validate -O3 --use IS_WASI=0 -b public/as-api.wasm",
    "build:engine": "asc assembly/uci.ts --runtime half --validate -O2 --runPasses flatten,simplify-globals-optimizing,local-cse,simplify-locals-notee-nostructure,licm,vacuum --use IS_WASI=1 --use abort=_abort -b build/engine.wasm -t build/engine.wast",
    "asbuild:debug": "asc assembly/index.ts --debug --use IS_WASI=0 -b public/as-api.wasm",
    "build": "npm run asbuild && npm run jsbuild",
    "jsbuild": "react-scripts build",
    "jsstart": "react-scripts start",
    "start": "npm run asbuild && npm run jsstart",
    "start:debug": "npm run asbuild:debug && npm run jsstart",
    "jstest": "react-scripts test",
    "test": "asp --verbose",
    "perft": "asp --verbose --nortrace -c as-pect-perft.config.js --file=perft.performance.ts",
    "perft:engine": "asp --verbose --nortrace -c as-pect-perft.config.js --file=engine.performance.ts",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@as-pect/cli": "^3.1.4",
    "assemblyscript": "0.9.4-nightly.20200314",
    "babel-plugin-macros": "^2.8.0",
    "gh-pages": "^2.2.0",
    "workerize-loader": "^1.1.0"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  }
}
